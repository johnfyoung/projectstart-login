{
  "name": "projectstart-login",
  "version": "1.0.0",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev:server": "rm -rf ./dist && webpack --mode development --config webpack.server.dev.config.js --watch",
    "dev": "concurrently \"npm run dev:server\" \"cd client && npm run start\"",
    "build": "rm -rf ./dist && webpack --mode production --config webpack.server.prod.config.js",
    "start": "node ./dist/server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm build && cd client && npm && npm build",
    "test": "jest __test__ --watch"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "eslint-plugin-babel": "^5.2.1",
    "express": "^4.17.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.2",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.1",
    "node-gravatar": "^1.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "concurrently": "^4.1.0",
    "file-loader": "^3.0.1",
    "nodemon": "^1.17.5",
    "nodemon-webpack-plugin": "^4.0.7",
    "supertest": "^3.4.2",
    "webpack": "4.19.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  }
}
